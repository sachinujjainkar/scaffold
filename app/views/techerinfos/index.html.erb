<p id="notice"><%= notice %></p>

<h1>Techerinfos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @techerinfos.each do |techerinfo| %>
      <tr>
        <td><%= techerinfo.name %></td>
        <td><%= techerinfo.address %></td>
        <td><%= link_to 'Show', techerinfo %></td>
        <td><%= link_to 'Edit', edit_techerinfo_path(techerinfo) %></td>
        <td><%= link_to 'Destroy', techerinfo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Techerinfo', new_techerinfo_path %>
